{
    "name": "wtsergo/laminas-db-driver-async",
    "description": "Async driver for laminas db",
    "require": {
        "php": "^8.1",
        "amphp/amp": "3.x@dev",
        "amphp/parallel": "2.x@dev",
        "laminas/laminas-db": "dev-wtsergo/2.20.0 as 2.20.0",
        "ext-pdo": "*"
    },
    "minimum-stability": "dev",
    "version": "1.0.0",
    "license": [
        "MIT"
    ],
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Wtsergo\\LaminasDbDriverAsync\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wtsergo/laminas-db"
        }
    ]
}
